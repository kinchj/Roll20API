      ];
      var charname = emName; 
      emCharacter = findObjs({type:'character',name:charname})[0];
      if(emCharacter){
        char = getObj('character',emCharacter.id);
        let existingAbilities = findObjs({type: 'ability', characterid: emCharacter.id}).filter(m => abilityList.map(n=>n[0]).includes(m.get('name')));
        let controlledby = char.get('controlledby'); if(controlledby) { char.set({ inplayerjournals: msg.playerid, }); }
        _.each(existingAbilities, function(abil) {
          abil.remove();
        });
        _.each(abilityList, function(item){
          createObj('ability',{
          characterid:char.get("id"),
          name:item[0],
          action:item[1],
          istokenaction:item[2]
          });
         });
        var existingAttributes = findObjs({type: 'attribute', characterid: emCharacter.id}).filter(m => attributeList.map(n=>n[0]).includes(m.get('name')));
        _.each(existingAttributes, function(attr) {
          attr.remove();
          });
        _.each(attributeList, function(item){
          createObj('attribute',{
          characterid:char.get("id"),
           name:item[0],
           current:item[1],
           max:item[2]
          });
        });
      sendChat(`${scriptName}`,`/w "${who}" EncounterMule "${charname}" was refreshed`);
      log(`EncounterMule "${charname}" was refreshed`);
      } else {
        let char = createObj('character',{
          name:charname,
          controlledby:'all',
          inplayerjournals: msg.playerid,
          })
        _.each(abilityList, function(item){
          createObj('ability',{
          characterid:char.get("id"),
          name:item[0],
          action:item[1],
          istokenaction:item[2]
          });
        })
        _.each(attributeList, function(item){
          createObj('attribute',{
          characterid:char.get("id"),
          name:item[0],
          current:item[1],
           max:item[2]
          });
        })
      sendChat(`${scriptName}`,`/w "${who}" EncounterMule "${charname}" was created`);
      log(`EncounterMule "${charname}" was created`);
      }
    }
    if('api'===msg.type && /^!createEncounterCollectionMacros(\b\s|$)/i.test(msg.content)){
    let who=(getObj('player',msg.playerid)||{get:()=>'API'}).get('_displayname');
      macroList =[
       [`NPCs`,`%{${emName}|EncounterFullMenu} &{noerror} `,`''`,false],
       [`NPCs1`,`%{${emName}|Encounter1Menu} &{noerror} `,`''`,false],
       [`NPCs2`,`%{${emName}|Encounter2Menu} &{noerror} `,`''`,false]
      ]
      emCollectionMacro = findObjs({type:'macro'})[0];
      if(emCollectionMacro){
        macro = getObj('macro',emCollectionMacro.id);
        let existingMacros = findObjs({type: 'macro'}).filter(m => macroList.map(n=>n[0]).includes(m.get('name')));
        _.each(existingMacros, function(macro) {
          macro.remove();
          });
        _.each(macroList, function(item){
          createObj('macro',{
          name:item[0],
          action:item[1],
          visibleto:item[2],
          playerid: msg.playerid,
          istokenaction:item[3]
          });
        });
    sendChat(`${scriptName}`,`/w "${who}" Encounter Collections Macros for "${emName}" were refreshed`);
    log(`Collections Macros for "${emName}" were refreshed`);
      } else {
      let macro = createObj('macro',{
        })
      _.each(macroList, function(item){
        createObj('macro',{
        name:item[0],
        action:item[1],
        visibleto: "",
        playerid: msg.playerid,
        istokenaction:true
        });
      })
    sendChat(`${scriptName}`,`/w "${who}" Encounter Collections Macros for "${emName}" were created`);
    log(`Collections Macros for "${emName}" were created`);
    }
    }
    if('api'===msg.type && /^!EncounterMule(\b\s|$)/i.test(msg.content)){        
    let who=(getObj('player',msg.playerid)||{get:()=>'API'}).get('_displayname');
      sendChat(`${scriptName}`,`/w "${who}" &{template:npcaction} {{rname=Encounter Mule Generator}} {{name=${emName}}} {{description=[Create/Refresh Encounter Mule](!createEncounterMule" style="text-decoration: none; background: none; background-color: transparent; border: none; padding: 0px; font-size: 12px; color: #7e2d40;)
