      ];
      var statMuleName = mmName; 
      mmCharacter = findObjs({type:'character',name:statMuleName})[0];
      if(mmCharacter){
        statchar = getObj('character',mmCharacter.id);
        statchar.set({
          bio: `You can read more information about this Statblock MacroMule at https://app.roll20.net/forum/post/11344849/update-d-and-d-5e-statblock-macro-mule-v3`,
        });
        let controlledby = statchar.get('controlledby'); if(controlledby) { statchar.set({ inplayerjournals: msg.playerid, }); }
        let existingAbilitiestoKeep = findObjs({type: 'ability', characterid: mmCharacter.id}).filter(m => !abilityList.map(n=>n[0]).includes(m.get('name')));
        let existingAbilities = findObjs({type: 'ability', characterid: mmCharacter.id});
        _.each(existingAbilities, function(abil) {
          abil.remove();
        });
        _.each(abilityList, function(item){
          createObj('ability',{
          characterid:statchar.get("id"),
          name:item[0],
          action:item[1],
          istokenaction:item[2]
          });
         });
        _.each(existingAbilitiestoKeep, function(item){
          createObj('ability',{
          characterid:statchar.get("id"),
          name:item.get("name"),
          action:item.get("action"),
          istokenaction:item.get("istokenaction")
          });
         });
        let existingAttributestoKeep = findObjs({type: 'attribute', characterid: mmCharacter.id}).filter(m => !attributeList.map(n=>n[0]).includes(m.get('name')));
        let existingAttributes = findObjs({type: 'attribute', characterid: mmCharacter.id});
        _.each(existingAttributes, function(attr) {
          attr.remove();
          });
        _.each(attributeList, function(item){
          createObj('attribute',{
          characterid:statchar.get("id"),
           name:item[0],
           current:item[1],
           max:item[2]
          });
         });
         _.each(existingAttributestoKeep, function(item){
          createObj('attribute',{
          characterid:statchar.get("id"),
          name:item.get("name"),
          current:item.get("current"),
          max:item.get("max")
          });
        });
      sendChat(`${scriptName}`,`/w "${who}" MacroMule "${statMuleName}" was refreshed`);
      log(`MacroMule "${statMuleName}" was refreshed`);
      } else {
        let statchar = createObj('character',{
          name:statMuleName,
          controlledby:'all',
          inplayerjournals: msg.playerid,
          bio: `You can read more information about this Encounter MacroMule at https://app.roll20.net/forum/post/11344849/update-d-and-d-5e-statblock-macro-mule-v3`,
          })
        _.each(abilityList, function(item){
          createObj('ability',{
          characterid:statchar.get("id"),
          name:item[0],
          action:item[1],
          istokenaction:item[2]
          });
        })
        _.each(attributeList, function(item){
          createObj('attribute',{
          characterid:statchar.get("id"),
          name:item[0],
          current:item[1],
           max:item[2]
          });
        })
      sendChat(`${scriptName}`,`/w "${who}" MacroMule "${statMuleName}" was created`);
      log(`MacroMule "${statMuleName}" was created`);
      }
    }
    if ('api'===msg.type && /^!createEncounterMule(\b\s|$)/i.test(msg.content) && playerIsGM(msg.playerid)) {
    attributeList =[
       [`ENCOUNTER CONFIG OPTIONS`,`v1.0`,`Defaults | Options`],
