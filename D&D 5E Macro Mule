        action:item[1],
        visibleto: "",
        playerid: msg.playerid,
        istokenaction:true
        });
      })
    sendChat(`${scriptName}`,`/w "${who}" Collections Macros for "${mmName}" were created`);
    log(`Collections Macros for "${mmName}" were created`);
    }
