       [`sel`,`@{selected|`],
       [`switch0`,`PC`],
       [`switch1`,`NPC`],
       [`PC0`,``],
       [`NPC0`,`no`],
       [`PC1`,`no`],
       [`NPC1`,``],
       [`show0`,`no`],
       [`showon`,``],
       [`showoff`,`no`],
       [`spellcaster`,`no`],
       [`spellcaster0`,`no`],
       [`spellcaster1`,``],
       [`spellcaster2`,``],
       [`spellcaster3`,``],
       [`spellcaster4`,``],
       [`spellcaster5`,``],
       [`spellcaster6`,``],
       [`spellcaster7`,``],
       [`spellcaster8`,``],
       [`spellcaster9`,``],
       [`spellcaster10`,``],
       [`spellcaster11`,``],
       [`spellcaster12`,``],
       [`spellcaster13`,``],
       [`spellcaster14`,``],
       [`spellcaster15`,``],
       [`spellcaster16`,``],
       [`spellcaster17`,``],
       [`spellcaster18`,``],
       [`spellcaster19`,``],
       [`spellcaster20`,``]
      ];
    abilityList =[
      [`${defaults.menuName}`,`%{${mmName}|Menu} &{noerror} `,false],
      [`${defaults.statsName}`,`%{${mmName}|Stats} &{noerror} `,true],
      [`${defaults.rollsName}`,`%{${mmName}|Rolls} &{noerror} `,true],
