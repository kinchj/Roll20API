    if ('api'===msg.type && /^!changemacromulename(\b\s|$)/i.test(msg.content)){
        let args = msg.content.split(/\s+/);
        let who=(getObj('player',msg.playerid)||{get:()=>'API'}).get('_displayname');
        let content = args.slice(1).join(' ');
        if (1 !== args.length && content === mmName ) {
        sendChat(`${scriptName}`,`/w "${who}" MacroMule name is "${mmName}" (not changed - same name)`);
         };
        if (1 !== args.length && content === '' ) {
        sendChat(`${scriptName}`,`/w "${who}" MacroMule name is "${mmName}" (not changed - blank)`);
         };
        if (1 === args.length) {
        sendChat(`${scriptName}`,`/w "${who}" MacroMule name is "${mmName}" (not changed - no arguments included)`);
        };        
        if (1 !== args.length && content !== '' && content !== mmName) {
        mmName = content;
        state.CreateMacroMule.mmName = mmName;
        sendChat(`${scriptName}`,`/w "${who}" MacroMule name is "${mmName}"`);
         };
      log(`MacroMule name is "${mmName}"`);
    }
