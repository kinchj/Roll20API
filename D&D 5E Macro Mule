      var charname = mmName; 
      mmCharacter = findObjs({type:'character',name:charname})[0];
      if(mmCharacter){
        char = getObj('character',mmCharacter.id);
        let existingAbilities = findObjs({type: 'ability', characterid: mmCharacter.id}).filter(m => abilityList.map(n=>n[0]).includes(m.get('name')));
        let controlledby = char.get('controlledby'); if(controlledby) { char.set({ inplayerjournals: msg.playerid, }); }
        _.each(existingAbilities, function(abil) {
          abil.remove();
        });
        _.each(abilityList, function(item){
          createObj('ability',{
          characterid:char.get("id"),
          name:item[0],
          action:item[1],
          istokenaction:item[2]
          });
         });
        var existingAttributes = findObjs({type: 'attribute', characterid: mmCharacter.id}).filter(m => attributeList.map(n=>n[0]).includes(m.get('name')));
        _.each(existingAttributes, function(attr) {
          attr.remove();
          });
        _.each(attributeList, function(item){
          createObj('attribute',{
          characterid:char.get("id"),
           name:item[0],
           current:item[1],
           max:item[2]
          });
        });
      sendChat(`${scriptName}`,`/w "${who}" MacroMule "${charname}" was refreshed`);
      log(`MacroMule "${charname}" was refreshed`);
      } else {
        let char = createObj('character',{
          name:charname,
          controlledby:'all',
          inplayerjournals: msg.playerid,
          })
        _.each(abilityList, function(item){
          createObj('ability',{
          characterid:char.get("id"),
          name:item[0],
          action:item[1],
          istokenaction:item[2]
          });
        })
        _.each(attributeList, function(item){
          createObj('attribute',{
          characterid:char.get("id"),
          name:item[0],
          current:item[1],
           max:item[2]
          });
        })
      sendChat(`${scriptName}`,`/w "${who}" MacroMule "${charname}" was created`);
      log(`MacroMule "${charname}" was created`);
      }
    }
    if('api'===msg.type && /^!createCollectionMacros(\b\s|$)/i.test(msg.content)){
    let who=(getObj('player',msg.playerid)||{get:()=>'API'}).get('_displayname');
      macroList =[
       [`${defaults.menuName}`,`%{${mmName}|Menu} &{noerror} `,`all`,false],
       [`${defaults.statsName}`,`%{${mmName}|Stats} &{noerror} `,`all`,true],
       [`${defaults.rollsName}`,`%{${mmName}|Rolls} &{noerror} `,`all`,true],
       [`${defaults.pcattacksName}`,`%{${mmName}|PCAttacks} &{noerror} `,`all`,true],
       [`${defaults.npcabilitiesName}`,`%{${mmName}|NPCAbilities} &{noerror} `,`''`,true],
       [`${defaults.spellsName}`,`%{${mmName}|Spells} &{noerror} `,`all`,true]
      ]
      mmCollectionMacro = findObjs({type:'macro'})[0];
      if(mmCollectionMacro){
        macro = getObj('macro',mmCollectionMacro.id);
        let existingMacros = findObjs({type: 'macro'}).filter(m => macroList.map(n=>n[0]).includes(m.get('name')));
        _.each(existingMacros, function(macro) {
          macro.remove();
          });
        _.each(macroList, function(item){
          createObj('macro',{
          name:item[0],
          action:item[1],
          visibleto:item[2],
          playerid: msg.playerid,
          istokenaction:item[3]
          });
        });
    sendChat(`${scriptName}`,`/w "${who}" Collections Macros for "${mmName}" were refreshed`);
    log(`Collections Macros for "${mmName}" were refreshed`);
      } else {
      let macro = createObj('macro',{
        })
      _.each(macroList, function(item){
        createObj('macro',{
        name:item[0],
        action:item[1],
        visibleto: "",
        playerid: msg.playerid,
        istokenaction:true
        });
      })
    sendChat(`${scriptName}`,`/w "${who}" Collections Macros for "${mmName}" were created`);
    log(`Collections Macros for "${mmName}" were created`);
    }
    }
    if('api'===msg.type && /^!macromule(\b\s|$)/i.test(msg.content)){        
    let who=(getObj('player',msg.playerid)||{get:()=>'API'}).get('_displayname');
      sendChat(`${scriptName}`,`/w "${who}" &{template:npcaction} {{rname=Macro Mule Generator}} {{name=${mmName}}} {{description=[Change Macro Mule Name](!changemacromulename ?{Macro Mule Name?|${mmName}}" style="text-decoration: none; background: none; background-color: transparent; border: none; padding: 0px; font-size: 12px; color: #7e2d40;)    
      [Create/Refresh Macro Mule](!createMacroMule" style="text-decoration: none; background: none; background-color: transparent; border: none; padding: 0px; font-size: 12px; color: #7e2d40;)
      [Create/Refresh Collection Macros](!createCollectionMacros" style="text-decoration: none; background: none; background-color: transparent; border: none; padding: 0px; font-size: 12px; color: #7e2d40;)
      [ ](#" style="border: 1px solid #7e2d40; margin: 1px; cursor: default; display: block;) [Macro Mule Forum Post](https://app.roll20.net/forum/post/11344849/update-d-and-d-5e-statblock-macro-mule-v3/" style="text-decoration: none; background: none; background-color: transparent; border: none; padding: 0px; font-size: 12px; color: #7e2d40;)}}`);
    }
      if('api'===msg.type && /^!self(\b\s|$)/i.test(msg.content)){
        const args = msg.content.split(/\s+/);
        const who=(getObj('player',msg.playerid)||{get:()=>'API'}).get('_displayname');
        let content = args.slice(1).join(' ');
          if(_.has(msg,'rolltemplate') && _.isString(msg.rolltemplate) && msg.rolltemplate.length){
              content = content.replace(/\{\{/,'&{template:'+msg.rolltemplate+'} {{');
          }
          if(1 === args.length) {
              return;
          } else {
              sendChat(`${who} (self whisper)`,`/w "${who}" ${content} `);
          }
          return;
      } 
  })
  log(`-=> ${scriptName} script enabled v${version} <=- Macro Mule Name is: ${mmName}`);
});
