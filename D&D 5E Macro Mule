       [`ü§∫PCs`,`%{${emName}|PCs_All} {{rname=PC Stats}} &{noerror}`,`''`,false,true]
      ]
      emCollectionMacro = findObjs({type:'macro'})[0];
      if(emCollectionMacro){
        macro = getObj('macro',emCollectionMacro.id);
        let existingMacros = findObjs({type: 'macro'}).filter(m => macroEncounterList.map(n=>n[0]).includes(m.get('name')));
        _.each(existingMacros, function(macro) {
          macro.remove();
          });
        _.each(macroEncounterList, function(item){
          createObj('macro',{
          name:item[0],
          action:item[1],
          visibleto: "",
          playerid: msg.playerid,
          istokenaction:item[3]
          });
        });
    sendChat(`${scriptName}`,`/w "${who}" Collections Encounter Macros for "${emName}" were refreshed`);
    log(`Collections Encounter Macros for "${emName}" were refreshed`);
      } else {
      let macro = createObj('macro',{
        })
      _.each(macroList, function(item){
        createObj('macro',{
        name:item[0],
        action:item[1],
        visibleto: "",
        playerid: msg.playerid,
        istokenaction:true
        });
      })
    sendChat(`${scriptName}`,`/w "${who}" Collections Encounter Macros for "${emName}" were created`);
    log(`Collections Encounter Macros for "${emName}" were created`);
    }
    }
    if('api'===msg.type && /^!macromule(\b\s|$)/i.test(msg.content) && playerIsGM(msg.playerid)) {        
      sendChat(`${scriptName}`,`/w "${who}" &{template:npcaction} {{rname=Macro Mule Generator ${version}}} {{name=${mmName} & ${emName}}} {{description=[Macro Mule Forum Post](https://app.roll20.net/forum/post/11344849/update-d-and-d-5e-statblock-macro-mule-v3/" style="text-decoration: none; background: none; background-color: transparent; border: none; padding: 0px; font-size: 12px; color: #7e2d40;)
