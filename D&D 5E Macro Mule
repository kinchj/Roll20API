d}9_spellname|max}; display: inline-block@{selected|repeating_spell-9_$9_spellname|max}; )[(@{selected|repeating_spell-9_$9_spellrange})&nbsp;](%{${mmName}|InfoLinkStyle} @{${mmName}|at}{${mmName}|perc}{${mmName}|@{${mmName}|at}{${mmName}|switch@{selected|npc}}UnpreparedLinkStyle}@{${mmName}|sel}repeating_spell-9_$@{selected|repeating_spell-9_$9_spellprepared}@{selected|repeating_spell-9_$9_spellprepared}9_spellname|max}; display: inline-block@{selected|repeating_spell-9_$9_spellname|max}; display: none@{selected|repeating_spell-9_$9_spellrange};)[&nbsp;](%{${mmName}|BreakLinkStyle} display: block@{selected|repeating_spell-9_$9_spellname|max}; )`,false`]
      ];
      var charname = mmName; 
      mmCharacter = findObjs({type:'character',name:charname})[0];
      if(mmCharacter){
        char = getObj('character',mmCharacter.id);
        var existingAbilities = findObjs({type: 'ability', characterid: mmCharacter.id});
        _.each(existingAbilities, function(abil) {
          abil.remove();
        });
        _.each(abilityList, function(item){
          createObj('ability',{
          characterid:char.get("id"),
          name:item[0],
          action:item[1],
          istokenaction:item[2]
          });
         }); 
        var existingAttributes = findObjs({type: 'attribute', characterid: mmCharacter.id});
        _.each(existingAttributes, function(attr) {
          attr.remove(); 
          }); 
        _.each(attributeList, function(item){
          createObj('attribute',{
          characterid:char.get("id"),
           name:item[0],
          current:item[1],
          }); 
        });
      } else {
        let char = createObj('character',{
          name:charname,
          controlledby:'all',
          })
        _.each(abilityList, function(item){
          createObj('ability',{
          characterid:char.get("id"),
          name:item[0],
          action:item[1],
          istokenaction:item[2]
          });
        })
        _.each(attributeList, function(item){
          createObj('attribute',{
          characterid:char.get("id"),
          name:item[0],
          current:item[1],
          }); 
        })
      }
      sendChat(`${scriptName}`,`/w gm MacroMule "${mmName}" was created`);
      log(`MacroMule "${mmName}" was created`);
    }
    if('api'===msg.type && /^!createCollectionMacros(\b\s|$)/i.test(msg.content) && playerIsGM(msg.playerid)){        
      macroList =[ 
       [`${menuName}`,`%{${mmName}|Menu} &{noerror} `,`all`,false],
       [`${statsName}`,`%{${mmName}|Stats} &{noerror} `,`all`,true],
       [`${rollsName}`,`%{${mmName}|Rolls} &{noerror} `,`all`,true],
       [`${pcattacksName}`,`%{${mmName}|PCAttacks} &{noerror} `,`all`,true],
       [`${npcabilitiesName}`,`%{${mmName}|NPCAbilities} &{noerror} `,`''`,true],
       [`${spellsName}`,`%{${mmName}|Spells} &{noerror} `,`all`,true]
      ]
      mmCollectionMacro = findObjs({type:'macro'})[0];
      if(mmCollectionMacro){
        macro = getObj('macro',mmCollectionMacro.id);
        let existingMacros = findObjs({type: 'macro'}).filter(m => macroList.map(n=>n[0]).includes(m.get('name')));
        _.each(existingMacros, function(macro) {
          macro.remove();
          });
        _.each(macroList, function(item){
          createObj('macro',{
          name:item[0],
          action:item[1],
          visibleto:item[2],
          playerid: msg.playerid,
          istokenaction:item[3]
          });
        });
      } else { 
      let macro = createObj('macro',{
        })
      _.each(macroList, function(item){
        createObj('macro',{
        name:item[0],
        action:item[1],
        visibleto: "",
        playerid: msg.playerid,
        istokenaction:true
        });
      })
    }
    sendChat(`${scriptName}`,`/w gm Collections Macros for "${mmName}" were created`);
    log(`Collections Macros for "${mmName}" were created`);
    } 
    if('api'===msg.type && /^!macromule(\b\s|$)/i.test(msg.content) && playerIsGM(msg.playerid)){        
      sendChat(`${scriptName}`,`/w gm &{template:npcaction} {{rname=Macro Mule Generator}} {{name=Macro Mule name: ${mmName}}} {{description=[Create Macro Mule](!createMacroMule" style="text-decoration: none; background: none; background-color: transparent; border: none; padding: 0px; font-size: 12px; color: #7e2d40;)
      [Create Collection Macros](!createCollectionMacros" style="text-decoration: none; background: none; background-color: transparent; border: none; padding: 0px; font-size: 12px; color: #7e2d40;)}}`);
    } 
      if('api'===msg.type && /^!self(\b\s|$)/i.test(msg.content)){
        const args = msg.content.split(/\s+/);
        const who=(getObj('player',msg.playerid)||{get:()=>'API'}).get('_displayname');
        var content = args.slice(1).join(' ');
          if(_.has(msg,'rolltemplate') && _.isString(msg.rolltemplate) && msg.rolltemplate.length){
              content = content.replace(/\{\{/,'&{template:'+msg.rolltemplate+'} {{');
          }
          if(1 === args.length) {
              return;
          } else {
              sendChat(`${who}`,`/w "${who}" ${content} `);
          }
          return;          
      } 
  })
  log(`-=> ${scriptName} script enabled v${version} <=- Macro Mule Name is: ${mmName}`);
},0))
