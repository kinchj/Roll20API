        let existingAbilities = findObjs({type: 'ability', characterid: mmCharacter.id}).filter(m => abilityList.map(n=>n[0]).includes(m.get('name')));
        _.each(existingAbilities, function(abil) {
          abil.remove();
        });
        _.each(abilityList, function(item){
          createObj('ability',{
          characterid:char.get("id"),
          name:item[0],
          action:item[1],
          istokenaction:item[2]
          });
         });
