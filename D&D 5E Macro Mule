  }
  if ( ! state.CreateMacroMule ) {
    state.CreateMacroMule = {
      mmName: defaults.mmName,
      emName: defaults.emName
    };
  }
  var mmName = state.CreateMacroMule.mmName || defaults.mmName;
  var emName = state.CreateMacroMule.emName || defaults.emName;
  on("chat:message",function(msg){
        let args = msg.content.split(/\s+/);
        let who=(getObj('player',msg.playerid)||{get:()=>'API'}).get('_displayname');
    if ('api'===msg.type && /^!changeStatblockMuleName(\b\s|$)/i.test(msg.content) && playerIsGM(msg.playerid)) {
        let content = args.slice(1).join(' ');
        if (1 !== args.length && content === mmName ) {
        sendChat(`${scriptName}`,`/w "${who}" MacroMule name is "${mmName}" (not changed - same name)`);
         };
        if (1 !== args.length && content === '' ) {
        sendChat(`${scriptName}`,`/w "${who}" MacroMule name is "${mmName}" (not changed - blank)`);
         };
        if (1 === args.length) {
        sendChat(`${scriptName}`,`/w "${who}" MacroMule name is "${mmName}" (not changed - no arguments included)`);
        };        
        if (1 !== args.length && content !== '' && content !== mmName) {
        mmName = content;
        state.CreateMacroMule.mmName = mmName;
        sendChat(`${scriptName}`,`/w "${who}" MacroMule name is "${mmName}"`);
         };
      log(`MacroMule name is "${mmName}"`);
    }
    if ('api'===msg.type && /^!changeEncounterMuleName(\b\s|$)/i.test(msg.content) && playerIsGM(msg.playerid)) {
        let content = args.slice(1).join(' ');
        if (1 !== args.length && content === emName ) {
        sendChat(`${scriptName}`,`/w "${who}" EncounterMule name is "${emName}" (not changed - same name)`);
         };
        if (1 !== args.length && content === '' ) {
        sendChat(`${scriptName}`,`/w "${who}" EncounterMule name is "${emName}" (not changed - blank)`);
         };
        if (1 === args.length) {
        sendChat(`${scriptName}`,`/w "${who}" EncounterMule name is "${emName}" (not changed - no arguments included)`);
        };        
        if (1 !== args.length && content !== '' && content !== emName) {
        emName = content;
        state.CreateMacroMule.emName = emName;
        sendChat(`${scriptName}`,`/w "${who}" EncounterMule name is "${emName}"`);
         };
      log(`EncounterMule name is "${emName}"`);
