var API_Meta = API_Meta || {};
API_Meta.CreateMacroMule = { offset: Number.MAX_SAFE_INTEGER, lineCount: -1 };
{ try { throw new Error(''); } catch (e) { API_Meta.CreateMacroMule.offset = (parseInt(e.stack.split(/\n/)[1].replace(/^.*:(\d+):.*$/, '$1'), 10) - (3)); } }
on('ready',()=>{
  const scriptName = "CreateMacroMule";
  const version = '3.1';
  var defaults = {
      mmName: 'MM',
      menuName: 'Menu',
      statsName: 'Stats',
      rollsName: 'Rolls',
      pcattacksName: 'PCAttacks',
      pctraitsName: 'PCTraits',
      pcproficienciesName: 'PCProficiencies',
      pcequipmentName: 'PCEquipment',
      pcbioName: 'PCBio',
      npcabilitiesName: 'NPCAbilities',
      spellsName: 'Spells',
  }
  if ( ! state.CreateMacroMule ) {
    state.CreateMacroMule = {
      mmName: defaults.mmName
    };
  }
  var mmName = state.CreateMacroMule.mmName;
  on("chat:message",function(msg){
    if ('api'===msg.type && /^!changemacromulename(\b\s|$)/i.test(msg.content)){
        let args = msg.content.split(/\s+/);
        let who=(getObj('player',msg.playerid)||{get:()=>'API'}).get('_displayname');
        let content = args.slice(1).join(' ');
        if (1 !== args.length && content === mmName ) {
        sendChat(`${scriptName}`,`/w "${who}" MacroMule name is "${mmName}" (not changed - same name)`);
         };
        if (1 !== args.length && content === '' ) {
        sendChat(`${scriptName}`,`/w "${who}" MacroMule name is "${mmName}" (not changed - blank)`);
         };
        if (1 === args.length) {
        sendChat(`${scriptName}`,`/w "${who}" MacroMule name is "${mmName}" (not changed - no arguments included)`);
        };        
        if (1 !== args.length && content !== '' && content !== mmName) {
        mmName = content;
        state.CreateMacroMule.mmName = mmName;
        sendChat(`${scriptName}`,`/w "${who}" MacroMule name is "${mmName}"`);
         };
      log(`MacroMule name is "${mmName}"`);
    }
    if ('api'===msg.type && /^!createMacroMule(\b\s|$)/i.test(msg.content)){
    let who=(getObj('player',msg.playerid)||{get:()=>'API'}).get('_displayname');
    attributeList =[
       [`NPCWhisper`,`!self`],
       [`PCWhisper`,`!self`],
       [`InfoButton`,`📜`],
       [`InspirationButton`,`🔆`],
