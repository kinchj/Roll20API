          _.each(attributeListNoerror, function(item){
            createObj('attribute',{
            characterid:char.get("id"),
            name:item[0],
            current:item[1],
            max:item[2]
            });
          })
      var tokObj = {
            imgsrc: 'https://s3.amazonaws.com/files.d20.io/images/402346418/wqhl4Xo1tWE-Rgsr2sO4Ig/thumb.png?172193749755',
