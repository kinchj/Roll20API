      macroEncounterList =[
       [`üë∫Names`,`%{${emName}|NPC_Names} {{rname=Name Menu}} &{noerror}`,`''`,false,true],
       [`üßåNPCs`,`%{${emName}|NPCs_All} &{noerror}`,`''`,false,true],
