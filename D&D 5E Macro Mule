            }
          let createtoken = createObj('graphic', tokObj);
          setDefaultTokenForCharacter(char,createtoken);
          createtoken.remove();
          log(`${npc4name} was created`);
          }
 
        var pc1name = `${emName} PCs 1-3`; 
